@{
    ViewData["Title"] = "Historical";
}
<h1>Historical</h1>

<div id="map" style="width:400px;height:400px;"></div>

<script>
    function myMap() {
        var mapCanvas = document.getElementById("map");
        var mapOptions = {
            center: new google.maps.LatLng(40.538686, -7.2677887), zoom: 17
        };
        var map = new google.maps.Map(mapCanvas, mapOptions);
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA1mPaYmlqhJ28dVsQT-_8bbNTjhY4h0NI&callback=myMap"></script>
